{
  "name": "zynmacro",
  "version": "1.0.3",
  "description": "natro macro revamped ui",
  "main": "electron/main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder --win --x64",
    "build:zip": "electron-builder --win --x64 --dir",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andrewzapps/zyn-macro.git"
  },
  "keywords": [],
  "author": "andrewzapps",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/andrewzapps/zyn-macro/issues"
  },
  "homepage": "https://github.com/andrewzapps/zyn-macro#readme",
  "build": {
    "appId": "com.andrewzapps.zynmacro",
    "productName": "Zyn Macro",
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": ["x64"]
        },
        {
          "target": "zip",
          "arch": ["x64"]
        }
      ],
      "icon": "electron/zyn2.png"
    },
    "files": [
      "electron/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "extraResources": [
      {
        "from": "lib",
        "to": "lib"
      },
      {
        "from": "paths",
        "to": "paths"
      },
      {
        "from": "patterns",
        "to": "patterns"
      },
      {
        "from": "submacros",
        "to": "submacros"
      },
      {
        "from": "nm_image_assets",
        "to": "nm_image_assets"
      },
      {
        "from": "settings",
        "to": "settings"
      },
      {
        "from": "START.bat",
        "to": "START.bat"
      },
      {
        "from": "LICENSE.md",
        "to": "LICENSE.md"
      },
      {
        "from": "README.md",
        "to": "README.md"
      }
    ],
    "directories": {
      "output": "dist"
    }
  },
  "devDependencies": {
    "electron": "^39.2.7",
    "electron-builder": "^26.4.0"
  },
  "dependencies": {
    "ini": "^6.0.0"
  }
}
